{
  "name": "Geocaching Utils",
  "description": "A collection of useful features to enhance geocaching.com",
  "version": "0.5.2",
  "permissions": [
    "storage",
    "https://maps.googleapis.com/maps/api/elevation/*"
  ],
  "options_page": "html/options.html",
  "content_scripts": [
    {
      "matches": ["https://www.geocaching.com/*"],
      "css": ["css/style.css"],
      "js": ["js/lib/jquery-3.1.0.min.js", "js/common.js", "js/elevationFeature.js", "js/friendFeatures.js", "js/start.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "img/spinner.gif"
  ],
  "browser_action": {
    "default_title": "Geocaching Utils",
    "default_icon": {
      "16": "img/appIcon16.png",
      "48": "img/appIcon48.png"
    }
  },
  "icons": {
    "16": "img/appIcon16.png",
    "48": "img/appIcon48.png",
    "128": "img/appIcon128.png"
  },
  "manifest_version": 2
}
