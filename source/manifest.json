{
  "name": "Geocaching Utils",
  "description": "A useful collection of features for geocaching.com",
  "version": "0.2",
  "permissions": [
    "activeTab",
    "https://maps.googleapis.com/maps/api/elevation/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.geocaching.com/*"],
      "css": ["css/style.css"],
      "js": ["js/lib/jquery-3.1.0.min.js", "js/coordinateHelper.js", "js/script.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Geocaching Utils",
    "default_icon": {
            "19": "img/tradi19.png",
            "38": "img/tradi38.png"
    }
  },
  "manifest_version": 2
}
