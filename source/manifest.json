{
  "name": "Geocaching Utils",
  "description": "A useful collection of features for geocaching.com",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.geocaching.com/*"],
      "css": ["style.css"],
      "js": ["jquery-3.1.0.min.js", "script.js"],
      "run_at": "document_end"
    }
  ],
  "externally_connectable": {
  "matches": ["https://maps.googleapis.com/maps/api/elevation/*"]
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
