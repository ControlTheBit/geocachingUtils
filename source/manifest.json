{
  "name": "Geocaching Utils",
  "description": "A collection of useful features for geocaching.com",
  "version": "0.4.1",
  "permissions": [
    "activeTab",
    "https://maps.googleapis.com/maps/api/elevation/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.geocaching.com/*"],
      "css": ["css/style.css"],
      "js": ["js/lib/jquery-3.1.0.min.js", "js/common.js", "js/elevation.js", "js/friendFeatures.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Geocaching Utils",
    "default_icon": {
            "16": "img/appIcon16.png",
            "48": "img/appIcon48.png",
            "128": "img/appIcon128.png"
    }
  },
  "web_accessible_resources": [
    "img/spinner.gif"
  ],
  "manifest_version": 2
}
